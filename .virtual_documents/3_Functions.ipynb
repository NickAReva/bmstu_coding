    print("Age: %d\n" % age)

describe_person_m("john")





def print_order(*order, **info):
    print("МУзыка")
    for key, value in sorted(info.items()):
        print(key, ": ", value)

    print("You choose: ")
    for item in order:
        print(" - ", item)

songs = ("a", "b", "c")
authors = {"author": "Sar", "birthday": "06/06/2020"}

print_order("a", "b", "c", author = "Sar", birthday = "06/06/2020")
        



